{
  "AWS": {
    "Profile": "MyGeneralProfile",
    "Region": "eu-west-2",
  },
  "MySecretsManagerConfig": {
    "Profile": "MySecretsProfile",
    "Region": "eu-west-1",
    "ProfilesLocation": "C:\\Users\\MyUser\\.aws\\",
    "PollingIntervalInSeconds": 60,
    "AcceptedSecretArns": [
      "arn:example:01-abcxyz",
      "arn:example:02",
      "unique-secret-name"
    ],
    "ListSecretsFilters": [
      {
        "Key": "Name",
        "Values": [ "Value1", "Value2" ]
      }
    ]
  }
}
